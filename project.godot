; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Rotas"
run/main_scene="res://Scenes/level_select_scene.tscn"
boot_splash/image="res://Sprites/splash.png"
config/icon="res://Sprites/icon.png"

[autoload]

ConfigManager="*res://Scripts/config_manager.gd"
GameDataManager="*res://Scripts/game_data_manager.gd"
Globals="*res://Scripts/globals.gd"
AudioManager="*res://Scripts/audio_manager.gd"

[display]

window/size/height=576
window/size/test_width=1024
window/size/test_height=576
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

fallback="en_GB"
translations=PoolStringArray( "res://locale/translations.pt_BR.translation", "res://locale/translations.en_GB.translation" )
translation_remaps={
}

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/directional_shadow/size=1024
quality/directional_shadow/size.mobile=512
environment/default_environment="res://default_env.tres"
